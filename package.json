{
  "name": "@softoika/schematics",
  "version": "1.0.1",
  "description": "A miscellaneous schematics collection for Angular",
  "scripts": {
    "build": "tsc -p tsconfig.json && scripts/post-build.sh",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --quiet --fix"
  },
  "keywords": [
    "schematics",
    "angular"
  ],
