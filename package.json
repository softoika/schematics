{
  "name": "@softoika/schematics",
  "version": "1.1.1",
  "description": "A miscellaneous schematics collection for Angular",
  "scripts": {
    "build": "tsc -p tsconfig.json && scripts/post-build.sh",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --quiet --fix"
  },
  "keywords": [
    "schematics",
    "angular"
  ],
  "author": "Ryo Hanafusa",
  "license": "MIT",
  "schematics": "./dist/collection.json",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@angular-devkit/core": "8.3.26",
    "@angular-devkit/schematics": "8.3.26",
    "@schematics/angular": "8.3.26"
  },
  "devDependencies": {
    "@types/jest": "25.1.5",
    "@types/node": "8.10.60",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "typescript": "3.7.5"
  },
  "homepage": "https://github.com/softoika/schematics",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softoika/schematics"
  },
  "bugs": {
    "url": "https://github.com/softoika/schematics/issues"
  }
}
