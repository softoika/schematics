{
  "name": "@softoika/schematics",
  "version": "1.1.2",
  "description": "A miscellaneous schematics collection for Angular",
  "scripts": {
    "build": "tsc -p tsconfig.json && scripts/post-build.sh",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --quiet --fix"
  },
  "keywords": [
    "schematics",
    "angular"
  ],
  "author": "Ryo Hanafusa",
  "license": "MIT",
  "schematics": "./dist/collection.json",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@angular-devkit/core": "11.2.14",
    "@angular-devkit/schematics": "11.2.14",
    "@schematics/angular": "11.2.14"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "8.10.66",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.29.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "26.6.3",
    "prettier": "2.3.2",
    "ts-jest": "26.5.6",
    "typescript": "4.3.2"
  },
  "homepage": "https://github.com/softoika/schematics",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softoika/schematics"
  },
  "bugs": {
    "url": "https://github.com/softoika/schematics/issues"
  }
}
